{
    "name": "wwwillian/laravel-json-crud",
    "description": "A package that creates a new development method with Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Matheus Alves",
            "email": "connect.malves@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.*",
        "webmozart/json": "^1.2.2",
        "webmozart/assert": "^1.3",
        "wwwillian/laravel-helpers": "dev-master",
        "daylerees/sanitizer": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wwwillian\\JsonCrud\\Commands\\CommandsServiceProvider",
                "Wwwillian\\JsonCrud\\Repositories\\RepositoryServiceProvider",
                "Wwwillian\\JsonCrud\\Providers\\EventServiceProvider"
            ],
            "aliases": []
        }
    },
    "autoload": {
        "psr-4": {
            "Wwwillian\\JsonCrud\\": "src/"
        }
    }
